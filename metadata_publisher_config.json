    "blog": {
      "label": "Blog",
      "markdown_visible": true,
      "html_visible": false
    }
  },
  "html_map": {
    "page_title": {
      "prefix": "<h1>",
      "postfix": "</h1>",
      "omit_meta": true
    },
    "page_subtitle": {
      "prefix": "<h2>",
      "postfix": "</h2>",
      "omit_meta": true
    }
  }
